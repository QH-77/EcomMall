<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="res_Theme" parent="Theme.AppCompat.Light.NoActionBar">
        <!--<item name="android:statusBarColor">@color/black</item>-->
        <item name="android:windowBackground">@color/res_windowBackground</item>
        <item name="colorPrimary">@color/res_colorPrimary</item>
        <item name="colorPrimaryDark">@color/res_colorPrimaryDark</item>

        <item name="android:textColor">@color/res_colorTextColor</item>
        <item name="android:textColorHint">@color/res_colorTextHintColor</item>
        <item name="colorAccent">@color/res_colorAccent</item>
        <!--<item name="toolbarStyle">@style/ClubToolbar</item>-->
    </style>

    <style name="res_TranslucentTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@color/res_transparent</item>
        　　
        　　<item name="android:windowIsTranslucent">true</item>
        　　
        　　<item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>
    </style>

    <!-- 标题 -->
    <style name="res_title">
        <item name="android:textColor">@color/gray_33</item>
        <item name="android:textSize">@dimen/res_title_text</item>
    </style>
    <!-- 主要文字-->
    <style name="res_text">
        <item name="android:textColor">@color/gray_33</item>
        <item name="android:textSize">@dimen/res_text</item>
    </style>
    <!-- 辅助文字 -->
    <style name="res_text_assist">
        <item name="android:textColor">@color/gray_b1</item>
        <item name="android:textSize">@dimen/res_text_assist</item>
    </style>

    <!-- 次要文字 -->
    <style name="res_text_secondary">
        <item name="android:textColor">@color/gray_33</item>
        <item name="android:textSize">@dimen/res_text_secondary</item>
    </style>
    <!-- 第三文字 -->
    <style name="res_text_assist_third">
        <item name="android:textColor">@color/gray_55</item>
        <item name="android:textSize">@dimen/res_text_assist</item>
    </style>
    <!-- 提示文字 -->
    <style name="res_text_hint">
        <item name="android:textColor">@color/gray_33</item>
        <item name="android:textSize">@dimen/res_text_hint</item>
    </style>
</resources>